<!-- Widget: Game Result -->
<Linkside className="widget card widget--sidebar widget-game-result">
	<div className="widget__title card__header card__header--has-btn">
		<h4>Last Game Results</h4>
		<button className="btn btn-default btn-outline btn-xs card-header__button js-switch-toggle"><span className="js-switch-txt" data-text-expand="Expand Stats" data-text-shrink="Shrink Stats">Expand Stats</span></button>
	</div>
	<div className="widget__content card__content">
		<!-- Game Score -->
		<div className="widget-game-result__section">
			<div className="widget-game-result__section-inner">
				<header className="widget-game-result__header">
					<h3 className="widget-game-result__title">Championship Quarter Finals</h3>
					<time className="widget-game-result__date" datetime="2016-03-24">Saturday, March 24th, 2016</time>
				</header>

				<div className="widget-game-result__main">
					<!-- 1st Team -->
					<div className="widget-game-result__team widget-game-result__team--first">
						<figure className="widget-game-result__team-logo">
							<Link href="#"><img src="{{../root}}assets/images/samples/logos/alchemists_last_game_results_small.png" alt=""></Link>
						</figure>
						<div className="widget-game-result__team-info">
							<h5 className="widget-game-result__team-name">Idaho Esports</h5>
							<div className="widget-game-result__team-desc">Elric Bros School</div>
						</div>
					</div>
					<!-- 1st Team / End -->

					<div className="widget-game-result__score-wrap">
						<div className="widget-game-result__score">
							<span className="widget-game-result__score-result widget-game-result__score-result--winner">107</span> <span className="widget-game-result__score-dash">-</span> <span className="widget-game-result__score-result widget-game-result__score-result--loser">102</span>
						</div>
						<div className="widget-game-result__score-label">Final Score</div>
					</div>

					<!-- 2nd Team -->
					<div className="widget-game-result__team widget-game-result__team--second">
						<figure className="widget-game-result__team-logo">
							<Link href="#"><img src="{{../root}}assets/images/samples/logos/sharks_last_game_results_small.png" alt=""></Link>
						</figure>
						<div className="widget-game-result__team-info">
							<h5 className="widget-game-result__team-name">Sharks</h5>
							<div className="widget-game-result__team-desc">Marine College</div>
						</div>
					</div>
					<!-- 2nd Team / End -->
				</div>
			</div>
		</div>
		<!-- Game Score / End -->

		<!-- Scoreboard -->
		<div className="widget-game-result__section">
			<div className="widget-game-result__table-stats">
				<div className="table-responsive">
					<table className="table table__cell-center table-thead-color">
						<thead>
							<tr>
								<th>Scoreboard</th>
								<th>1</th>
								<th>2</th>
								<th>3</th>
								<th>4</th>
								<th>T</th>
							</tr>
						</thead>
						<tbody>
							<tr>
								<th>Idaho Esports</th>
								<td>30</td>
								<td>31</td>
								<td>22</td>
								<td>24</td>
								<td>107</td>
							</tr>
							<tr>
								<th>Sharks</th>
								<td>22</td>
								<td>34</td>
								<td>20</td>
								<td>26</td>
								<td>102</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
		</div>
		<!-- Scoreboard / End -->

		<!-- Game Statistics -->
		<div className="widget-game-result__section">
			<header className="widget-game-result__subheader card__subheader card__subheader--sm card__subheader--nomargins">
				<h5 className="widget-game-result__subtitle">Game Statistics</h5>
			</header>
			<div className="widget-game-result__section-inner">
				<!-- Progress: Assists -->
				<div className="progress-double-wrapper">
					<h6 className="progress-title">Assists</h6>
					<div className="progress-inner-holder">
						<div className="progress__digit progress__digit--left">22</div>
						<div className="progress__double">
							<div className="progress">
								<div className="progress__bar progress__bar-width-60" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
							<div className="progress">
								<div className="progress__bar progress__bar--info progress__bar-width-80" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
						</div>
						<div className="progress__digit progress__digit--right progress__digit--highlight">36</div>
					</div>
				</div>
				<!-- Progress: Assists / End -->

				<!-- Progress: Rebounds -->
				<div className="progress-double-wrapper">
					<h6 className="progress-title">Rebounds</h6>
					<div className="progress-inner-holder">
						<div className="progress__digit progress__digit--left progress__digit--highlight">35</div>
						<div className="progress__double">
							<div className="progress">
								<div className="progress__bar progress__bar-width-90" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
							<div className="progress">
								<div className="progress__bar progress__bar--info progress__bar-width-40" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
						</div>
						<div className="progress__digit progress__digit--right">18</div>
					</div>
				</div>
				<!-- Progress: Rebounds / End -->

				<!-- Progress: Steals -->
				<div className="progress-double-wrapper">
					<h6 className="progress-title">Steals</h6>
					<div className="progress-inner-holder">
						<div className="progress__digit progress__digit--left">14</div>
						<div className="progress__double">
							<div className="progress">
								<div className="progress__bar progress__bar-width-30" role="progressbar" aria-valuenow="30" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
							<div className="progress">
								<div className="progress__bar progress__bar--info progress__bar-width-70" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100"></div>
							</div>
						</div>
						<div className="progress__digit progress__digit--right progress__digit--highlight">24</div>
					</div>
				</div>
				<!-- Progress: Steals / End -->
			</div>
		</div>
		<!-- Game Statistics / End -->

		<div className="widget-game-result__extra-stats">

			<!-- Game MVP -->
			<div className="widget-game-result__section">
				<header className="widget-game-result__subheader card__subheader card__subheader--sm card__subheader--nomargins">
					<h5 className="widget-game-result__subtitle">Game MVP</h5>
				</header>
				<div className="widget-game-result__section-inner">
					<div className="player-details">
						<div className="player-details__info">
							<figure className="player-details__photo">
								<Link href="#"><img src="{{../root}}assets/images/samples/stats_player_02.jpg" alt=""></Link>
							</figure>
							<div className="player-details__info-holder">
								<h5 className="player-details__name"><Link href="#">Mark Stevens</Link></h5>
								<span className="player-details__position">1st Power Forward</span>
							</div>
						</div>
						<div className="player-details__stats">
							<div className="player-details__circular circular">
								<div className="circular__bar" data-percent="80">
									<span className="circular__percents">36<small>Pts</small></span>
								</div>
							</div>
							<div className="player-details__circular circular">
								<div className="circular__bar" data-percent="60">
									<span className="circular__percents">18<small>Reb</small></span>
								</div>
							</div>
							<div className="player-details__circular circular">
								<div className="circular__bar" data-percent="70">
									<span className="circular__percents">29<small>Ast</small></span>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
			<!-- Game MVP / End -->

		</div>
	</div>
</Linkside>
<!-- Widget: Game Result / End -->
